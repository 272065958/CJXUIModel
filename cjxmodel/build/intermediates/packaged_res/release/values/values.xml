<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="cjx_background_color">#f7f7f7</color>
    <color name="cjx_colorAccent">#FF4081</color>
    <color name="cjx_colorPrimary">#1c9cf6</color>
    <color name="cjx_colorPrimaryDark">#0088cc</color>
    <color name="cjx_divider_color">#d8d8d8</color>
    <color name="cjx_item_pressed_color">#33bdbdbd</color>
    <color name="cjx_shadow_color">#33000000</color>
    <color name="cjx_tab_background_color">#fafafa</color>
    <color name="cjx_tab_text_color">#202020</color>
    <color name="cjx_text_deep_color">#101010</color>
    <color name="cjx_text_hint_color">#b6b6b6</color>
    <color name="cjx_text_main_color">#333</color>
    <color name="cjx_text_secondary_color">#717171</color>
    <color name="cjx_text_title_color">#9e9e9f</color>
    <color name="invalid_color">#cccccc</color>
    <dimen name="auto_margin">8dp</dimen>
    <dimen name="bigger_margin">12dp</dimen>
    <dimen name="block_line_height">2dp</dimen>
    <dimen name="button_height">40dp</dimen>
    <dimen name="divider_height">0.6dp</dimen>
    <dimen name="double_fab_margin">32dp</dimen>
    <dimen name="elevation_bottom_bar">2dp</dimen>
    <dimen name="elevation_title_bar">2dp</dimen>
    <dimen name="empty_icon">100dp</dimen>
    <dimen name="fab_margin">16dp</dimen>
    <dimen name="half_bigger_margin">6dp</dimen>
    <dimen name="header_size">64dp</dimen>
    <dimen name="icon_size">30dp</dimen>
    <dimen name="item_height">48dp</dimen>
    <dimen name="pixels_height">1px</dimen>
    <dimen name="tab_icon_size">20dp</dimen>
    <dimen name="tab_text_size">11sp</dimen>
    <dimen name="tab_text_top_margin">23dp</dimen>
    <dimen name="text_margin">4dp</dimen>
    <dimen name="text_size_big">20sp</dimen>
    <dimen name="text_size_big_big">34sp</dimen>
    <dimen name="text_size_middle">13sp</dimen>
    <dimen name="text_size_more_small">10sp</dimen>
    <dimen name="text_size_six">16sp</dimen>
    <dimen name="text_size_small">12sp</dimen>
    <dimen name="toolbar_height">52dp</dimen>
    <item name="constraint_content" type="id"/>
    <item name="view_id" type="id"/>
    <item name="view_tag" type="id"/>
    <string name="app_name">cjxModel</string>
    <string name="load_more">正在加载…</string>
    <style name="button_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/button_height</item>
        <item name="android:textSize">@dimen/text_size_six</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="loading_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
    <style name="loading_progress_style" parent="@android:style/Widget.ProgressBar">
    </style>
    <style name="slide_dialog">
        <item name="android:windowExitAnimation">@anim/bottom_to_top</item>
        <item name="android:windowEnterAnimation">@anim/top_to_bottom</item>
    </style>
    <style name="tabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">12sp</item>
    </style>
    <style name="tablayout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/button_height</item>
        <item name="android:background">@color/cjx_background_color</item>
    </style>
    <style name="toolbar_title" parent="@style/Base.TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">16sp</item>
    </style>
</resources>